ALTER SESSION SET SQL_TRACE = TRUE;
-- Run your MERGE statement
ALTER SESSION SET SQL_TRACE = FALSE;

BEGIN
    MERGE INTO employee_rating er
    USING employee e
    ON (e.employee_id = er.employee_id)
    WHEN MATCHED THEN
    UPDATE SET er.network_login = e.login;
EXCEPTION
    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE(DBMS_UTILITY.FORMAT_ERROR_BACKTRACE);
        RAISE;
END;

SELECT name, type, line, text
FROM user_source
WHERE UPPER(text) LIKE '%20000%';