fields @timestamp, @message
| filter @message like /abcd-hfjfb-jfj-.*/
| parse @message /abcd-hfjfb-jfj-(?<variable>[^\s]+)/
| filter variable == "xyz"
| sort @timestamp asc
| stats count() as errorCount by bin(4m), @message
| filter errorCount > 0 and @message like /error/
| filter timestamp < @timestamp + 240000 and timestamp > @timestamp